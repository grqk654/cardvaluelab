---
import Base from "./Base.astro";
const { title, description } = Astro.props;
---
<Base title={title} description={description}>
  <div class="hero">
    <div class="kicker"><span class="dot"></span><span class="badge">Calculator</span></div>
    <h1 class="h1">{title}</h1>
    <p>{description}</p>
  </div>
  <slot />
</Base>
